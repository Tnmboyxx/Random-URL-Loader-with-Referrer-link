{
    "name": "Random URL Loader",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Loads random URLs from a list at random intervals with referer forwarding.",   
    "permissions": [
      "tabs",
      "storage",
      "alarms"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
}
  